{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "35rem",
			"wideSize": "41.25rem"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "0.625rem",
					"slug": "tiny"
				},
				{
					"name": "X-Small",
					"size": "1.25rem",
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": "1.875rem",
					"slug": "sm"
				},
				{
					"name": "Medium",
					"size": "clamp(1.875rem, 5vw, 3.125rem)",
					"slug": "md"
				},
				{
					"name": "Large",
					"size": "clamp(1.875rem, 7vw, 4.375rem)",
					"slug": "lg"
				},
				{
					"name": "X-Large",
					"size": "clamp(3.125rem, 7vw, 5.625rem)",
					"slug": "xl"
				},
				{
					"name": "XX-Large",
					"size": "clamp(4.375rem, 10vw, 8.75rem)",
					"slug": "xxl"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/inter/inter-400-normal.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/inter/inter-900-normal.woff2"
							]
						}
					],
					"fontFamily": "Inter, sans-serif",
					"name": "Inter",
					"slug": "inter"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Instrument Serif\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/instrument-serif/instrument-serif-400-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Instrument Serif\"",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/instrument-serif/instrument-serif-400-italic.woff2"
							]
						}
					],
					"fontFamily": "\"Instrument Serif\", serif",
					"name": "Instrument Serif",
					"slug": "instrument-serif"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"slug": "sm",
					"size": "1rem"
				},
				{
					"name": "Medium",
					"slug": "md",
					"size": "1.25rem"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.275rem"
					},
					"name": "Large",
					"size": "1.275rem",
					"slug": "lg"
				},
				{
					"fluid": {
						"max": "1.75rem",
						"min": "1.3rem"
					},
					"name": "X-Large",
					"size": "1.3rem",
					"slug": "xl"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.325rem"
					},
					"name": "XX-Large",
					"size": "1.325rem",
					"slug": "xxl"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "1.375rem"
					},
					"name": "XXX-Large",
					"size": "1.375rem",
					"slug": "xxxl"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--xs)"
					}
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--sm)"
					},
					"margin": {
						"top": "var(--wp--preset--spacing--xs)"
					}
				}
			},
			"core/list-item": {
				"spacing": {
					"margin": {
						"top": "0.5em",
						"bottom": "0.5em"
					}
				}
			},
			"core/gallery": {
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--instrument-serif)",
					"lineHeight": "1.2"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxxl)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxl)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--md)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "var(--wp--preset--font-size--sm)",
				"right": "var(--wp--preset--font-size--sm)",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"lineHeight": "1.75"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "Header"
		},
		{
			"area": "footer",
			"name": "Footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}